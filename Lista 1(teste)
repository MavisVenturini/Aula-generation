programa
{
	
	funcao inicio()
	{
		real nota1
		real nota2
		real nota3
		

		escreva ("Digite a primeira nota")
		leia(nota1)
		escreva ("Digite a segunda nota")
		leia(nota2)
		escreva ("Digite a terceira nota")
		leia(nota3)

		real notafinal = ((nota1*2)+(nota2*3)+(nota3*5))/10
		escreva("nota final")
		
	}
}
